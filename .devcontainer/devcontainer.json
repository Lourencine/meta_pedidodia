{
    "name": "Python Dev Container",
    "build": {
      "dockerfile": "Dockerfile"
    },
    "settings": {
      "python.defaultInterpreterPath": "/usr/local/bin/python"
    },
    "extensions": [
      "ms-python.python",
      "ms-toolsai.jupyter"
    ],
    "postCreateCommand": "pip install -r requirements.txt",
    "remoteUser": "vscode"
  }
  